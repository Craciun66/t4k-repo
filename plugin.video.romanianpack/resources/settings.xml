        <setting id="searchtype" enable="eq(-2,true)" type="select" label="Cautare în:" values="Ambele|Site-uri|Torrent" visible="eq(-2,true)" default="Ambele" />
    </category>
    <category label="Trakt">
        <setting type="lsep" label="Trakt.tv" />
