<?xml version="1.0" encoding="UTF-8"?>
<addons>
<addon id="plugin.program.t4kbuild" name="T4K Build" version="1.0.6" provider-name="Tips4Kodi">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>program executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
		<summary lang="en">T4K Build</summary>
		<description lang="en">Un Kodi Build cu continut romanesc...pentru info: www.tips4kodi.net</description>
		<disclaimer lang="en">Atentie: Continutul build-ului nu este creat de echipa Kodi, ci de dezvoltatori neautorizati terti. Orice incalcare a drepturilor de autor, defectiuni si probleme legate de acestea nu sunt, prin urmare, atribuite echipei Kodi si nici noua tips4kodi.net.</disclaimer>
		<platform>all</platform>
		<facebook>https://www.facebook.com/tips4kodi.net/</facebook>
		<website>https://tips4kodi.net</website>
	</extension>
</addon>

<addon id="plugin.program.t4kwizard" name="[COLOR dodgerblue][B]T4K[/B][/COLOR] Wizard" version="0.3.0" provider-name="unicsuc">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="startup.py" start="startup" />
  <extension point="xbmc.addon.metadata">
    <summary lang="en">[COLOR dodgerblue]T4K[/COLOR] Wizard</summary>
    <description lang="en">[COLOR dodgerblue]T4K[/COLOR] Wizard allows you to get access to the latest [COLOR dodgerblue]T4K[/COLOR] Custom Builds.</description>
    <platform>all</platform>
  </extension>
</addon>

