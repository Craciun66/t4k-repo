<addon id="script.t4k.news" version="1.0.0"
       name="T4K News" provider-name="tips4kodi">
  <requires>
  	<import addon="xbmc.python" version="2.1.0"/>
  	<import addon="script.module.addon.common" optional="true" />
  </requires>
  <extension point="xbmc.python.script" library="default.py">
    <provides>executable</provides>
  </extension>
  <extension point="xbmc.service" library="service.py" start="startup">
  </extension>
  <extension point="xbmc.addon.metadata">
  	<summary>Tips4Kodi NEWS</summary>
  	<description>Cu ajutorul acestui plugin veti primi notificari din partea lui Tips4Kodi.</description>
  	<disclaimer>Nu ne asumam nici o raspundere pentru erorile acestui plugin.</disclaimer>
  	<language>en</language>
  	<platform>all</platform>
  	<website>https://tips4kodi.net</website>
  	<source></source>
  	<email></email>
  </extension>
</addon>
</addons>
